<h2><%= link_to portf_item.title, portfolio_show_path(portf_item.id) %></h2>
<p><%= portf_item.subtitle %></p>
<p><%= portf_item.body %></p>
<%= image_tag portf_item.thumb_image unless portf_item.thumb_image.nil? %>
<br>
<%= link_to "Edit", edit_portfolio_path(portf_item.id) %>
<%= link_to "Delete Portfolio Item", portfolio_path(portf_item.id), method: :delete, data:{confirm: "Are you sure?"} %>
